#!/bin/sh
PHP=`which php`
exec $PHP -C -q -d output_buffering=1 "$0" "$@"
<?php



if(isset($argv)){
  ob_end_clean();
  $args = getopt("l:i:o:");
  if(substr($args["i"],0,1!="/")) $args["i"] = getcwd()."/".$args["i"];
  if(substr($args["o"],0,1!="/")) $args["o"] = getcwd()."/".$args["o"];
  $group = new PhroccoGroup($args);
}

